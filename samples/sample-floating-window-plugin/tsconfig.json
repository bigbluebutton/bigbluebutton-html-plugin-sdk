{
    "compilerOptions": {
        "outDir": "./dist-tsc/",
        "noImplicitAny": true,
        "module": "es6",
        "target": "es5",
        "skipLibCheck": true,
        "jsx": "react",
        "allowJs": true,
        "moduleResolution": "node"
    },
    "include": ["src/*", "src/components/window/floating-personal-notes", "src/components/window/header", "src/components/sample-floating-window-plugin-item"],
    "paths": {
        "*": ["node_modules"]
    },
    "exclude": [
        "node_modules"
    ]
}
